#!/bin/bash

if [ "$1" == "debug" ]
then
    set -x
fi

SCRIPT=`basename $0`
SCRIPT_DIR=`dirname $0`
INSTALL_DIR_UNIX="/usr/bin"
INSTALL_DIR_WINDOWS="/cygdrive/c"
EXTRACT_DIR="/tmp/extract_asrm_client_$$"
# skip $SKIP no. of lines to "get at" the archive
SKIP=`awk '/^__ASRM_CLIENT_PKGS__/ { print NR + 1; exit 0; }' $0`
PLATFORM=$(uname)
if [[ $PLATFORM =~ "CYGWIN" ]]
then
    PLATFORM="windows"
elif [[ $PLATFORM =~ "HP" ]]
then
    PLATFORM="HPUX"
fi

if [[ $PLATFORM == "windows" ]]
then
    INSTALL_DIR=$INSTALL_DIR_WINDOWS
elif [[ $PLATFORM == "Linux" ]]
then
    INSTALL_DIR=$INSTALL_DIR_UNIX
    LSB_RELEASE=`which lsb_release`
else
    INSTALL_DIR=$INSTALL_DIR_UNIX
fi

ASRM_CLIENT_WRAPPER="asrm_client_wrapper"
ASRM_CLIENT_LIB="asrm_clientlib.py"
ASRM_CLIENT="asrm_client.py"
ASRM_CLIENT_CONF="asrm_client.conf"
ASRM_CLIENT_CTL="asrm_client_ctl"
ASRM_CLIENT_SERVICE="asrm_client_ctl.service"
SunOS_RC_DIR="/lib/svc/method/"
HPUX_RC_DIR="/sbin/init.d/"
AIX_RC_DIR="/etc/rc.d/init.d"
rhel5_RC_DIR="/etc/init.d" # common for rhel6, sles11
sles11_RC_DIR="/etc/init.d" # common for rhel6, sles11
ubuntu14_RC_DIR="/etc/init.d"
rhel7_LIB_SYSTEMD_DIR="/usr/lib/systemd/system" # common for sles12
rhel7_ETC_SYSTEMD_DIR="/etc/systemd/system" # common for sles12
ubuntu16_LIB_SYSTEMD_DIR="/usr/lib/systemd/user"
ubuntu16_ETC_SYSTEMD_DIR="/etc/systemd/system"
WINDOWS_SRVANY="srvany.exe"
WINDOWS_SYSTEM32_DIR="/cygdrive/c/Windows/System32"
CREATE_ASRM_CLIENT_SERVICE="create_asrm_client_service.bat"

function _get_distro() {
    output=`$LSB_RELEASE -i`
    if [[ $output =~ "RedHat" ]]
    then
        DISTRO="rhel"
    elif [[ $output =~ "Oracle" ]]
    then
        DISTRO="rhel"   # OEL6,CentOS and RHEL config routines are same !
    elif [[ $output =~ "CentOS" ]]
    then
        DISTRO="rhel"   # OEL6,CentOS and RHEL config routines are same !
    elif [[ $output =~ "EnterpriseEnterpriseServer" ]]
    then
        DISTRO="rhel"   # OEL5 and RHEL config routines are same !
    elif [[ $output =~ "SUSE" ]]
    then
        DISTRO="sles"
    elif [[ $output =~ "Ubuntu" ]]
    then
        DISTRO="ubuntu"
    else
        DISTRO=""
    fi
    echo $DISTRO
}

function _get_release() {
    echo `$LSB_RELEASE -r| awk -F ":" '{print $2}'|awk -F "." '{print $1}'|awk '{$1=$1};1'`
}

function _get_distro_release() {
    if [[ $PLATFORM =~ "windows" ]]
    then
        echo "windows"
    elif [[ $PLATFORM =~ "Linux" ]]
    then
        echo $(_get_distro)$(_get_release)
    else
        echo $PLATFORM
    fi
}

DISTRO=$(_get_distro_release)


function runcmd() {
    $1 >/dev/null 2>&1
    if [ $? != 0 ]
    then
        echo "ERROR: command[$1] is failed to execute !"
        exit 1
    fi 
}

function _install_daemon_pkg() {
    apt-get -y install daemon >/dev/null
}

function _install_client() {

    # STEP 1 : create install & extract directories
    mkdir -p $INSTALL_DIR $EXTRACT_DIR

    # STEP 2 : extract files in temporary dir
    if [ $PLATFORM == "AIX" ] || [ $PLATFORM == "SunOS" ] || [ $PLATFORM == "HPUX" ]
    then
        SKIP=`awk '/^__ASRM_CLIENT_PKGS__/ { print NR + 1; exit 0; }' $0`
        (tail +${SKIP} ${SCRIPT_DIR}/${SCRIPT} | gzip -c -d | (cd "${EXTRACT_DIR}" && tar xf -)) 2> /dev/null
    else
        tail -n +${SKIP} ${SCRIPT_DIR}/${SCRIPT} | tar xzf -  -C $EXTRACT_DIR 2>/dev/null
    fi

    #if [ `ls $EXTRACT_DIR|wc -l` != "6" ]
    #then
    #    echo "ERROR:   Failed to extract files to $EXTRACT_DIR !"
    #    exit 1
    #fi

    # STEP 3 : copy files to install directory
    \cp -rp $EXTRACT_DIR/$ASRM_CLIENT $INSTALL_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT to $INSTALL_DIR !"
        exit 1
    fi

    \cp -rp $EXTRACT_DIR/$ASRM_CLIENT_LIB $INSTALL_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT_LIB to $INSTALL_DIR !"
        exit 1
    fi
    
    test -e $INSTALL_DIR/$ASRM_CLIENT_CONF
    if [ $? != 0 ]
    then
        \cp -rp $EXTRACT_DIR/$ASRM_CLIENT_CONF $INSTALL_DIR/
        if [ $? != 0 ]
        then
            echo "ERROR:        Failed to copy $ASRM_CLIENT_CONF to $INSTALL_DIR !"
            exit 1
        fi
    fi


}

function _configure_rc_script_rhel5() {
    ASRM_CLIENT_CTL_SPECIFIC="${ASRM_CLIENT_CTL}_${DISTRO}"
    # STEP : copy init script "asrm_client_ctl" to /etc/init.d
    \cp $EXTRACT_DIR/$ASRM_CLIENT_CTL_SPECIFIC $rhel5_RC_DIR/$ASRM_CLIENT_CTL
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT_CTL to $rhel5_RC_DIR !"
        exit 1
    fi

    # STEP : copy client wrapper script to "asrm_client_ctl" to /etc/init.d
    \cp $EXTRACT_DIR/$ASRM_CLIENT_WRAPPER $INSTALL_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT_WRAPPER to $INSTALL_DIR !"
        exit 1
    fi

    \cp $EXTRACT_DIR/$ASRM_CLIENT_LIB $INSTALL_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT_LIB to $INSTALL_DIR !"
        exit 1
    fi

    # STEP : chkconfig --add asrm_client
    CMD="chkconfig --add $ASRM_CLIENT_CTL"
    runcmd "$CMD"

    # STEP : chkconfig --level 2345  asrm_client on
    CMD="chkconfig --level 2345  $ASRM_CLIENT_CTL on"
    runcmd "$CMD"
}

function _configure_rc_script_rhel6() {
    _configure_rc_script_rhel5
}

function _configure_rc_script_rhel7() {
    # STEP : copy service script "asrm_client_ctl.service" to /usr/lib/systemd/system/
    \cp $EXTRACT_DIR/$ASRM_CLIENT_SERVICE $rhel7_LIB_SYSTEMD_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT_SERVICE to $rhel7_LIB_SYSTEMD_DIR !"
        exit 1
    fi

    # STEP : Link asrm client service to systemd
    #CMD="ln -sf $rhel7_LIB_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE $rhel7_ETC_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE"
    \cp $rhel7_LIB_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE $rhel7_ETC_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE
    if [ $? != 0 ]
    then
       echo "ERROR:    Failed to copy [$rhel7_LIB_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE]  to [$rhel7_ETC_SYSTEMD_DIR] !"
       exit 1
    fi

    # STEP : systemctl daemon-reload
    CMD="systemctl daemon-reload"
    runcmd "$CMD"

    # STEP : systemctl enable asrm_client
    CMD="systemctl enable $ASRM_CLIENT_SERVICE"
    runcmd "$CMD"

    # STEP : systemctl start asrm_client
    CMD="systemctl start $ASRM_CLIENT_SERVICE"
    runcmd "$CMD"
}

