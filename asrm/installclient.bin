function _configure_rc_script_sles12() {
    # STEP : copy service script "asrm_client_ctl.service" to /usr/lib/systemd/system/
    \cp $EXTRACT_DIR/$ASRM_CLIENT_SERVICE $rhel7_LIB_SYSTEMD_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT_SERVICE to $rhel7_LIB_SYSTEMD_DIR !"
        exit 1
    fi

    # NOTE: only difference between sles12 and rhel7 is, sles12 does not require asrm_client_ctl.service
    # to copied at /usr/lib/systemd/system or /etc/systemd/system
    # STEP : Link asrm client service to systemd
    #CMD="ln -sf $rhel7_LIB_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE $rhel7_ETC_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE"
    #\cp $rhel7_LIB_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE $rhel7_ETC_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE
    #if [ $? != 0 ]
    #then
    #   echo "ERROR:    Failed to copy [$rhel7_LIB_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE]  to [$rhel7_ETC_SYSTEMD_DIR] !"
    #   exit 1
    #fi

    # STEP : systemctl daemon-reload
    CMD="systemctl daemon-reload"
    runcmd "$CMD"

    # STEP : systemctl enable asrm_client
    CMD="systemctl enable $ASRM_CLIENT_SERVICE"
    runcmd "$CMD"

    # STEP : systemctl start asrm_client
    CMD="systemctl start $ASRM_CLIENT_SERVICE"
    runcmd "$CMD"
}

function _configure_rc_script_ubuntu14() {
    ASRM_CLIENT_CTL_SPECIFIC="${ASRM_CLIENT_CTL}_${DISTRO}"
    # STEP : check if daemon package is installed
    dpkg -l daemon >/dev/null 2>&1
    if [ $? != 0 ]
    then
        echo "Installing daemon package"
        _install_daemon_pkg
        if [ $? != 0 ]
        then
            echo "ERROR:    Failed to install daemon package"
        fi
    fi

    # STEP : copy init script "asrm_client_ctl" to /etc/init.d
    \cp $EXTRACT_DIR/$ASRM_CLIENT_CTL_SPECIFIC $ubuntu14_RC_DIR/$ASRM_CLIENT_CTL
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT_CTL_SPECIFIC to $ubuntu14_RC_DIR !"
        exit 1
    fi

    # STEP : copy client wrapper script to "asrm_client_wrapper" to /usr/bin
    \cp $EXTRACT_DIR/$ASRM_CLIENT_WRAPPER $INSTALL_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT_WRAPPER to $INSTALL_DIR !"
        exit 1
    fi

    \cp $EXTRACT_DIR/$ASRM_CLIENT_LIB $INSTALL_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT_LIB to $INSTALL_DIR !"
        exit 1
    fi

    # STEP : update-rc.d asrm_client_ctl defaults
    CMD="update-rc.d asrm_client_ctl defaults"
    runcmd "$CMD"
}

function _configure_rc_script_ubuntu16() {
    # STEP : copy service script "asrm_client_ctl.service" to /usr/lib/systemd/system/
    \cp $EXTRACT_DIR/$ASRM_CLIENT_SERVICE $ubuntu16_LIB_SYSTEMD_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy $ASRM_CLIENT_SERVICE to $ubuntu16_LIB_SYSTEMD_DIR !"
        exit 1
    fi

    # STEP : Copy asrm client service to /etc/systemd
    CMD="/bin/cp -f $ubuntu16_LIB_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE $ubuntu16_ETC_SYSTEMD_DIR/$ASRM_CLIENT_SERVICE"
    runcmd "$CMD"

    # STEP : systemctl daemon-reload
    CMD="systemctl daemon-reload"
    runcmd "$CMD"

    # STEP : systemctl enable asrm_client
    CMD="systemctl enable $ASRM_CLIENT_SERVICE"
    runcmd "$CMD"

    # STEP : systemctl start asrm_client
    CMD="systemctl start $ASRM_CLIENT_SERVICE"
    runcmd "$CMD"
}

function _configure_rc_script_SunOS() {
    ASRM_CLIENT_CTL_SPECIFIC="${ASRM_CLIENT_CTL}_${PLATFORM}"
    runcmd "cp $EXTRACT_DIR/$ASRM_CLIENT_CTL_SPECIFIC $SunOS_RC_DIR/$ASRM_CLIENT_CTL"

    cat <<EOF > /var/svc/manifest/system/asrm_client_ctl.xml
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service_bundle SYSTEM "/usr/share/lib/xml/dtd/service_bundle.dtd.1">
<service_bundle type="manifest" name="asrm_client_ctl">
<service name="system/asrm_client_ctl" type="service" version="1">
<create_default_instance enabled="false"/>
<single_instance/>
<dependency name="net-loopback" grouping="require_all" restart_on="none" type="service">
<service_fmri value="svc:/network/loopback"/>
</dependency>
<dependency name="net-physical" grouping="require_all" restart_on="none" type="service">
<service_fmri value="svc:/network/physical"/>
</dependency>
<exec_method type="method" name="start" exec="/lib/svc/method/asrm_client_ctl %m" timeout_seconds="30"></exec_method>
<exec_method type="method" name="restart" exec="/lib/svc/method/asrm_client_ctl %m" timeout_seconds="30"></exec_method>
<exec_method type="method" name="stop" exec="/lib/svc/method/asrm_client_ctl %m" timeout_seconds="30"></exec_method>
<template>
<common_name>
<loctext xml:lang="C">ASRM Service</loctext>
</common_name>
</template>
</service>
</service_bundle>
EOF
    
    runcmd "svccfg validate /var/svc/manifest/system/asrm_client_ctl.xml"
    runcmd "svccfg import /var/svc/manifest/system/asrm_client_ctl.xml"
    runcmd "svcadm enable asrm_client_ctl"
    runcmd "svcs -xv asrm_client_ctl"
}

function _configure_rc_script_HPUX() {
    ASRM_CLIENT_CTL_SPECIFIC="${ASRM_CLIENT_CTL}_${PLATFORM}"
    runcmd "cp $EXTRACT_DIR/$ASRM_CLIENT_CTL_SPECIFIC $HPUX_RC_DIR/$ASRM_CLIENT_CTL"
    cd /sbin/rc2.d/
    runcmd "ln -sf $HPUX_RC_DIR/$ASRM_CLIENT_CTL S900${ASRM_CLIENT_CTL}"
    cd /sbin/rc1.d/
    runcmd "ln -sf $HPUX_RC_DIR/$ASRM_CLIENT_CTL K500${ASRM_CLIENT_CTL}"
}

function _configure_rc_script_AIX() {
    ASRM_CLIENT_CTL_SPECIFIC="${ASRM_CLIENT_CTL}_${PLATFORM}"
    runcmd "cp $EXTRACT_DIR/$ASRM_CLIENT_CTL_SPECIFIC $AIX_RC_DIR/$ASRM_CLIENT_CTL"
    cd /etc/rc.d/rc2.d/
    runcmd "ln -sf $AIX_RC_DIR/$ASRM_CLIENT_CTL  S99${ASRM_CLIENT_CTL}"
    runcmd "ln -sf $AIX_RC_DIR/$ASRM_CLIENT_CTL  K99${ASRM_CLIENT_CTL}"
}

function _configure_rc_script_Linux() {
    _configure_rc_script_${DISTRO}
}

function _configure_rc_script_windows() {
    # STEP 1 : copy srvany.exe to /cygdrive/c/Windows/System32
    /bin/cp -f $EXTRACT_DIR/$WINDOWS_SRVANY $WINDOWS_SYSTEM32_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy[$WINDOWS_SRVANY] to [$WINDOWS_SYSTEM32_DIR/] !"
        exit 1
    fi

    # STEP 2 : copy cmd /c c:\\create_asrm_client_service.bat to /cygdrive/c/
    /bin/cp -f $EXTRACT_DIR/$CREATE_ASRM_CLIENT_SERVICE $INSTALL_DIR/
    if [ $? != 0 ]
    then
        echo "ERROR:    Failed to copy[$CREATE_ASRM_CLIENT_SERVICE] to [$INSTALL_DIR/] !"
        exit 1
    fi
     
    # STEP 2 : Execute script "create_asrm_client_service.bat" to configure asrm_client.py as a service
    #          cmd /c c:\\create_asrm_client_service.bat
    CMD="cmd /c c:\\\\${CREATE_ASRM_CLIENT_SERVICE}"
    runcmd "$CMD"
}

#function _populate_client_conf() {
#}

function _cleanup() {
    # STEP 4 : Remove extract directory
    rm -rf $EXTRACT_DIR
}

echo "installing ASRM client "
_install_client
echo "making ASRM client as an RC script "
_configure_rc_script_${PLATFORM}
#_populate_client_conf
echo "cleaning up temporary files"
_cleanup
echo "success"

exit 0
